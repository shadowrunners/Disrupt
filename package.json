{
	"name": "scarlett-engine",
	"version": "1.0.0",
	"scripts": {
		"build": "tsup && npm link"
	},
	"files": [
		"dist"
	],
	"tsup": {
		"dts": true,
		"bundle": false,
		"treeshake": true,
		"target": "node16",
		"format": [
			"esm",
			"cjs"
		],
		"entry": [
			"src/**/*.ts",
			"index.ts"
		]
	},
	"author": "test",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/test/scarlett.git"
	},
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"axios": "^1.5.1",
		"egoroof-blowfish": "^4.0.1",
		"ffmpeg-static": "^5.2.0",
		"libsodium-wrappers": "^0.7.13",
		"m3u8stream": "^0.8.6",
		"prism-media": "^1.3.5"
	},
	"devDependencies": {
		"@types/node": "^20.8.4",
		"eslint": "^8.51.0",
		"terser": "^5.21.0",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	}
}
